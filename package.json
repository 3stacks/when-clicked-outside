{
  "name": "@lukeboyle/when-clicked-outside",
  "version": "2.0.2",
  "description": "Micro-library to trigger a callback when a user clicks anything BUT your target element.",
  "main": "index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "prepublish": "npm run build-all",
    "build": "rollup --config rollup.config.js",
    "build-demo": "npm run build-css && npm run build-html && npm run build-js",
    "build-all": "npm run build && npm run build-demo",
    "build-js": "browserify ./demo/src/js/index.js -o ./demo/build/app.js -t [ babelify --presets [ es2015 react ] ]",
    "build-css": "node-sass ./demo/src/sass/style.scss --output ./demo/build/css/",
    "build-html": "cp ./demo/src/html/index.html ./demo/build/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://3stacks@github.com/3stacks/when-clicked-outside.git"
  },
  "author": "Luke Boyle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3stacks/when-clicked-outside/issues"
  },
  "homepage": "https://github.com/3stacks/when-clicked-outside#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "node-sass": "^3.8.0",
    "rollup": "^0.26.1",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-node-resolve": "^1.7.0",
    "rollup-plugin-npm": "^1.4.0"
  },
  "dependencies": {
    "babel-runtime": "^6.9.2"
  }
}
